i.   Department_Table (dept_id(PK), dept_name)
ii.  Insurance_Table (insurance_id(PK), isurance_name)
iii. Office_Table(office_id(PK), office_name, place)
iv.  Employee_Info_Table(id(PK), DOJ, salary)
v.   Employee_Table( emp_id(PK), name, immediate_head_id)
vi.  Employee_Office_Table(id(PK), emp_id(FK), office_id(FK)
vii. Employee_Table_Department_Table(emp_id(PK),dept_id(FK))
viii. Employee_Table_insurance_table(emp_id(PK),insurance_id(FK))
ix. Employee_Table_employee_info(emp_id(PK),employee_info(FK))
